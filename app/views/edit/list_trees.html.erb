<% content_for :sidebar do
  render partial: 'edit/sidebar'
end  %>

<h1>Trees in data set</h1>

<% if @list.empty? %>
<div class="muted">
	No trees in dataset.
</div>
<% else %>
<ul>
	<% @list.each do |t| %>
	<li>
		<%= render partial: 'tree/tree', object: t %>
	</li>
	<% end %>
</ul>
<% end %>

<%= render partial: "edit/sql_results" %>
